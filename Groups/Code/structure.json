        "from": {
            "table": "Social\\Groups\\Code\\Tables\\Groups",
            "alias": "sg",
            "name": "#__social_groups"
        },
        "select": [
            "sg.id",
            "sg.title",
            "sg.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "sg.description",
