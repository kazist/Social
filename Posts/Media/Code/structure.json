    "query": {
        "from": {
            "table": "Social\\Posts\\Media\\Code\\Tables\\Media",
            "alias": "spm",
            "name": "#__social_posts_media"
        },
        "select": [
            "spm.id",
            "spm.post_id",
            "sp.post AS post_id_post",
            "spm.media_id",
            "mm.title AS media_id_title",
            "mm.file AS media_id_file",
            "spm.created_by",
            "spm.date_created",
            "spm.modified_by",
            "spm.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Social\\Posts\\Code\\Tables\\Posts",
                "alias": "sp",
                "name": "#__social_posts",
                "on": "spm.post_id = sp.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "spm.media_id = mm.id"
            }
        ]
    }
