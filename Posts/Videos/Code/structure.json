    "query": {
        "from": {
            "table": "Social\\Posts\\Videos\\Code\\Tables\\Videos",
            "alias": "spv",
            "name": "#__social_posts_videos"
        },
        "select": [
            "spv.id",
            "spv.post_id",
            "sp.post AS post_id_post",
            "spv.type",
            "spv.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "spv.file",
            "mm_6078.title AS file_title",
            "mm_6078.file AS file_file",
            "spv.youtube_code",
            "spv.embed_code",
            "spv.created_by",
            "spv.date_created",
            "spv.modified_by",
            "spv.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Social\\Posts\\Code\\Tables\\Posts",
                "alias": "sp",
                "name": "#__social_posts",
                "on": "spv.post_id = sp.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "spv.image = mm.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm_6078",
                "name": "#__media_media",
                "on": "spv.file = mm_6078.id"
            }
        ]
    }
