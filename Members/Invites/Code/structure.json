    "query": {
        "from": {
            "table": "Social\\Members\\Invites\\Code\\Tables\\Invites",
            "alias": "smi",
            "name": "#__social_members_invites"
        },
        "select": [
            "smi.id",
            "smi.name",
            "smi.email",
            "smi.user_id",
            "uu.name AS user_id_name",
            "smi.friend_id",
            "uu_1280.name AS friend_id_name",
            "smi.created_by",
            "smi.date_created",
            "smi.modified_by",
            "smi.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "smi.user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_1280",
                "name": "#__users_users",
                "on": "smi.friend_id = uu_1280.id"
            }
        ]
    }
