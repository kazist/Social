    "query": {
        "from": {
            "table": "Social\\Members\\Code\\Tables\\Members",
            "alias": "sm",
            "name": "#__social_members"
        },
        "select": [
            "sm.id",
            "sm.avatar",
            "mm.title AS avatar_title",
            "mm.file AS avatar_file",
            "sm.cover",
            "mm_1250.title AS cover_title",
            "mm_1250.file AS cover_file",
            "sm.dob",
            "sm.gender",
            "sm.user_id",
            "uu.name AS user_id_name",
            "sm.country_id",
            "sc.name AS country_id_name",
            "sm.location_id",
            "sr.name AS location_id_name",
            "sm.city",
            "sm.inviter_id",
            "uu_8508.name AS inviter_id_name",
            "sm.id_passport",
            "sm.phone",
            "sm.address",
            "sm.points",
            "sm.created_by",
            "sm.date_created",
            "sm.modified_by",
            "sm.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "sm.avatar = mm.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm_1250",
                "name": "#__media_media",
                "on": "sm.cover = mm_1250.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "sm.user_id = uu.id"
            },
            {
                "table": "Setup\\Countries\\Code\\Tables\\Countries",
                "alias": "sc",
                "name": "#__setup_countries",
                "on": "sm.country_id = sc.id"
            },
            {
                "table": "Setup\\Regions\\Code\\Tables\\Regions",
                "alias": "sr",
                "name": "#__setup_regions",
                "on": "sm.location_id = sr.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8508",
                "name": "#__users_users",
                "on": "sm.inviter_id = uu_8508.id"
            }
        ]
    }
