
{{ set_assets('applications/Social/Addons/Members/assets/css/style.css') }}

<ul class="social-members">
    {% if members|length > 0 %}
        {% for member in members %}
            <li>
                <a href="{{ generate_url('social.members.detail', {'id':member.user_id}) }}"  title="{{ member.user_name}}">
                    <img src="{{ resized_image(member.avatar_image, 48,48) }}" alt="{{ member.user_name}}" title="{{ member.user_name}}">
                </a>
            </li>
        {% endfor %}
    {% endif %}
</ul>

<div class="clearfix"></div>